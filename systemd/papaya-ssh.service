[Unit]
Description=PapayaMeter reverse SSH tunnel service
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
User=mritunjay
WorkingDirectory=/home/mritunjay/Desktop/PapayaMeter
ExecStart=/usr/bin/python3 /home/mritunjay/Desktop/PapayaMeter/services/ssh_service.py
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal
SyslogIdentifier=papaya-ssh

[Install]
WantedBy=multi-user.target
